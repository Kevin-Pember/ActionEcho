{
    "manifest_version": 3,
    "name": "AutoMade",
    "description": "Automate actions in chrome",
    "version": "1.0",
    "permissions": ["scripting", "activeTab","storage","unlimitedStorage"],
    "content_scripts": [
      {
        "matches": ["https://*/*"],
        "js": ["JS/inject.js"]
      }
    ],
    "background": {
      "service_worker": "js/background.js"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": "icon.png"
    }
  }