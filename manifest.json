{
    "manifest_version": 3,
    "name": "AutoMade",
    "description": "Automate actions in chrome",
    "version": "1.0",
    "permissions": ["scripting", "activeTab","storage","unlimitedStorage","favicon", "webNavigation","tabs"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["https://*/*"],
        "js": ["libraries/webcomponents-bundle.js","JS/inject.js"]
      }
    ],
    "background": {
      "service_worker": "js/background.js"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": "icon.png"
    },
    "web_accessible_resources": [
      {
        "resources": ["_favicon/*"],
        "matches": ["<all_urls>"],
        "extension_ids": ["*"]
      }
    ]
  }